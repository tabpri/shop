<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
    <class name="net.malta.model.AppFunction" table="APP_FUNCTION" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="java.lang.Integer" unsaved-value="null">
            <column name="ID" sql-type="INTEGER"/>
            <generator class="native">
            </generator>
        </id>
        <property name="functionName" type="java.lang.String">
            <column name="FUNCTION_NAME" not-null="true" unique="false" sql-type="VARCHAR2(100)"/>
        </property>
        <property name="functionURI" type="java.lang.String">
            <column name="FUNCTION_URI" not-null="true" unique="false" sql-type="VARCHAR2(255)"/>
        </property>
    </class>
</hibernate-mapping>