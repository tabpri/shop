<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
    <class name="net.malta.model.Role" table="ROLE" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="java.lang.Integer" unsaved-value="null">
            <column name="ID" sql-type="INTEGER"/>
            <generator class="native">
            </generator>
        </id>
        <property name="roleName" type="java.lang.String">
            <column name="ROLE_NAME" not-null="true" unique="false" sql-type="VARCHAR2(50)"/>
        </property>
        <property name="roleDescription" type="java.lang.String">
            <column name="ROLE_DESCRIPTION" not-null="true" unique="false" sql-type="VARCHAR2(255)"/>
        </property>
    </class>
</hibernate-mapping>