<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
    <class name="net.malta.model.PaymentStatus" table="PAYMENT_STATUS" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="java.lang.Integer" unsaved-value="null">
            <column name="ID" sql-type="INTEGER"/>
            <generator class="native">
            </generator>
        </id>
        <property name="paymentStatusString" type="java.lang.String">
            <column name="PAYMENT_STATUS" not-null="true" unique="false" sql-type="VARCHAR(100)"/>
        </property>
        <property name="acs" type="java.lang.String">
            <column name="PAYMENTGATEWAY_ACS" unique="false" sql-type="VARCHAR(50)"/>
        </property>
        <property name="MD" type="java.lang.String">
            <column name="PAYMENTGATEWAY_MD" unique="false" sql-type="VARCHAR(1000)"/>
        </property>
        <property name="paReq" type="java.lang.String">
            <column name="PAYMENTGATEWAY_PAREQ" unique="false" sql-type="VARCHAR(1000)"/>
        </property>
        <property name="paRes" type="java.lang.String">
            <column name="PAYMENTGATEWAY_PARES" unique="false" sql-type="VARCHAR(1000)"/>
        </property>        
        <property name="acsURL" type="java.lang.String">
            <column name="PAYMENTGATEWAY_ACSURL" unique="false" sql-type="VARCHAR(1000)"/>
        </property>
        <property name="transactionReference" type="java.lang.String">
            <column name="TRANSACTION_REFERENCE" unique="false" sql-type="VARCHAR(100)"/>
        </property>
        <property name="transactionDate" type="java.util.Date">
            <column name="TRANSACTION_DATE" unique="false" sql-type="DATETIME"/>
        </property>
        <property name="orderId" type="java.lang.String">
            <column name="ORDER_ID" unique="false" sql-type="VARCHAR(255)"/>
        </property>
        <property name="purchaseId" type="int">
            <column name="PURCHASE_FK" unique="false" sql-type="INTEGER"/>
        </property>		
    </class>
</hibernate-mapping>